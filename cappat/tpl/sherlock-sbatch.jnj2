#!/bin/bash
#
#SBATCH -n {{nodes}}
#SBATCH -t {{time}}
#SBATCH -p {{partition}}
{% if job_name %}
#SBATCH -J {{job_name}}
{% endif %}
{% if mincpus %}
#SBATCH --mincpus={{mincpus}}
{% endif %}
{% if mem_per_cpu %}
#SBATCH --mem-per-cpu={{mem_per_cpu}}
{% endif %}
{% if qos %}
#SBATCH -q {{qos}}
{% endif %}
{% if job_log %}
#SBATCH -o {{job_log}}.out
#SBATCH -e {{job_log}}.err
{% endif %}
{% if modules %}{% for m in modules %}
module {{m}}
{% endfor %}{% endif %}
srun {{commandline}}